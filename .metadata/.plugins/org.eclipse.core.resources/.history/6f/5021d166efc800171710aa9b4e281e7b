import java.io.*;
import java.util.*;

import javax.print.attribute.standard.NumberUpSupported;

public class P2FamilyTrees {

	public static void main(String[] args) throws Exception {
		Scanner in = new Scanner( new BufferedReader(new FileReader(new File("src/2017/DATA11.txt"))));
		for(int x = 0; x < 10; x++) {
			int numberOfLines = in.nextInt();
			for(int y = 0; y < numberOfLines; y++) {
				int [] firstID = csv(in.nextLine());
				
			}
		}
	}

	public static int[] csv(String stuff) {
		String[] preNums = new String[1];;
		if(stuff.contains(".")) {
			preNums = null;
			preNums = stuff.split(".");
		}
		int[] numbers = new int[preNums.length];
		
		for(int i = 0; i < preNums.length; i ++) {
			System.out.println(preNums[i]);
			numbers[i] = Integer.parseInt(preNums[i]);
		}
		return numbers;
	}

}
